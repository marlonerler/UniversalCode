command:
    rename: greetPersonByName = greetPerson;
end cmd;

myNumber = 123;

interface Person:
    string name,
    int age,
end ifc;

constant Person: jane = object:
    name = 'Jane',
    age = 42;

constant Person[]: people = array:
    john, jane;

function greetPerson: string name, returns string:
    constant string: greetingString = 'Hello, $name!';
    return: greetingString;
end fn;

each people, take person:
    constant string: greetingString = [greetPerson person.name];
    [console.log greetingString];
end loop;